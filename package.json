{
  "name": "tocu-graph-api",
  "version": "0.1.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec babel-node server.js",
    "dev": "docker-compose -f docker-compose.dev.yml up",
    "prod": "docker-compose -f docker-compose.prod.yml up -d",
    "build": "babel . --ignore node_modules,test,flow-typed,interfaces --out-dir dist",
    "serve": "node dist/server.js",
    "update-schema": "babel-node --presets es2015,stage-0 ./scripts/updateSchema.js",
    "lint": "eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mongodump-dev": "docker exec -it graphapi_mongo_1 /bin/bash -c 'mongodump -d dev -o /backup'",
    "mongorestore-dev": "docker exec -it graphapi_mongo_1 /bin/bash -c 'mongorestore -d dev /backup/dev'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/tocu/graph-api.git"
  },
  "keywords": [
    "tocu",
    "graph",
    "api"
  ],
  "author": "Hieu Tran <hieu.tranduc@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "csv-parse": "^1.1.7",
    "eslint": "^3.7.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^2.0.0",
    "flow-bin": "^0.33.0",
    "flow-graphql": "^0.7.0",
    "iflow-lodash": "^1.1.27",
    "iflow-moment": "^1.1.13"
  },
  "dependencies": {
    "@tranduchieu/graphql-custom-types": "^0.6.8",
    "@tranduchieu/short-parse-id": "0.0.4",
    "aws-sdk": "^2.6.6",
    "axios": "^0.14.0",
    "bluebird": "^3.4.6",
    "cors": "^2.7.1",
    "dataloader": "^1.2.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphiql": "^0.7.4",
    "graphql": "^0.8.2",
    "graphql-relay": "^0.4.2",
    "jimp": "^0.2.27",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "node-uuid": "^1.4.7",
    "parse": "^1.9.2",
    "parse-dashboard": "^1.0.19",
    "parse-server": "^2.2.24",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "tabletojson": "^0.4.0"
  },
  "bugs": {
    "url": "https://gitlab.com/tocu/graph-api/issues"
  },
  "homepage": "https://gitlab.com/tocu/graph-api#README"
}
